{
  "name": "angular-particle",
  "version": "1.0.4",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n angular-particle -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "test": "tsc && karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryuKKu-/angular-particle"
  },
  "author": {
    "name": "Luc Raymond",
    "email": "ryukku.raymond@gmail.com"
  },
  "keywords": [
    "angular2",
    "angular4",
    "angular",
    "particlesjs",
    "particle",
    "typescript",
    "ng2",
    "particles"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryuKKu-/angular-particle/issues"
  },
  "devDependencies": {
    "@angular/core": "~8.1.2",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.4.0",
    "gulp-rollup": "^2.16.2"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
